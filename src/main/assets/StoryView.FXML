<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<fx:root type="javafx.scene.layout.VBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
         minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="20.0"
         xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text fx:id="storyText" strokeType="OUTSIDE" strokeWidth="0.0" text="Story" textAlignment="CENTER"
              wrappingWidth="550.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Text>
        <Text fx:id="resultText" strokeType="OUTSIDE" strokeWidth="0.0" text="Result"/>
        <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#onConfirm" text="Confirm"/>
    </children>
</fx:root>
